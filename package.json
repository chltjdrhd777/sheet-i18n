{
  "name": "sheet-i18n",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "vitest",
    "plop": "plop",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset-canary": "pnpm changeset pre enter canary",
    "versioning": "pnpm changeset version --since=origin/feature",
    "versioning-canary": "pnpm versioning && pnpm changeset pre exit",
    "publish": "turbo run build && pnpm changeset publish",
    "publish-canary": "turbo run build && pnpm changeset publish --tag canary",
    "prepare": "husky"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/js": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "commitlint-plugin-function-rules": "^4.0.1",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "mkdirp": "^3.0.1",
    "plop": "^4.0.1",
    "prettier": "^3.4.2",
    "standard-version": "^9.5.0",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "5.5.4",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
