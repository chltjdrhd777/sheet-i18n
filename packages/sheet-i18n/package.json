{
  "name": "sheet-i18n",
  "version": "1.1.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./exporter": {
      "require": "./dist/exporter/index.js",
      "import": "./dist/exporter/index.mjs",
      "types": "./dist/exporter/index.d.ts"
    },
    "./react": {
      "require": "./dist/react/index.js",
      "import": "./dist/react/index.mjs",
      "types": "./dist/react/index.d.ts"
    }
  },
  "keywords": [
    "sheet-i18n",
    "spreadsheet",
    "i18n",
    "translation"
  ],
  "author": {
    "name": "devAnderson",
    "email": "chltjdrhd777@gmail.com",
    "url": "https://github.com/chltjdrhd777"
  },
  "license": "ISC",
  "dependencies": {
    "@sheet-i18n/exporter": "workspace:*",
    "@sheet-i18n/react": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^6.0.0",
    "typescript": "^5.0.0",
    "@sheet-i18n/typescript-config": "workspace:*"
  }
}
