{
  "name": "@sheet-i18n/react-client",
  "version": "0.2.0",
  "description": "a client package for react modules used by sheet-i18n",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": {
    "name": "devAnderson",
    "email": "chltjdrhd777@gmail.com",
    "url": "https://github.com/chltjdrhd777"
  },
  "license": "ISC",
  "dependencies": {
    "@sheet-i18n/shared-utils": "workspace:*",
    "@sheet-i18n/errors": "workspace:*",
    "@sheet-i18n/react-core": "workspace:*",
    "react": "^19.0.0",
    "react-intl": "^7.0.4"
  },
  "devDependencies": {
    "@sheet-i18n/typescript-config": "workspace:*",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2"
  }
}
